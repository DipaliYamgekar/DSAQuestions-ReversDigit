    "rev_num = 0\n",
    "base_pos = 1\n",
    "\n",
    "def reversDigits(num):\n",
    "\tglobal rev_num\n",
    "\tglobal base_pos\n",
    "\tif(num > 0):\n",
    "\t\treversDigits((int)(num / 10))\n",
    "\t\trev_num += (num % 10) * base_pos\n",
    "\t\tbase_pos *= 10\n",
    "\treturn rev_num\n",
    "\n",
    "\n",
    "\n",
    "num = 567\n",
    "print(\"Reverse of no. is \",\n",
    "\treversDigits(num))"
